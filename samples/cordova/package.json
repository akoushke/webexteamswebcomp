{
	"name": "Cordova_Web_Components",
	"version": "1.0.0",
	"scripts": {
		"install": "cordova prepare",
		"build:ios": "cordova build ios",
		"build:android": "cordova build android",
		"build:electron": "cordova build electron",
		"build:osx": "cordova build osx",
		"build:all": "npm run build:ios && npm run build:android && npm run build:osx",
		"run:ios": "cordova run ios",
		"run:android": "cordova run android",
		"run:electron": "cordova run electron",
		"run:osx": "cordova run osx",
		"run:all": "npm run run:ios && npm run run:android && npm run run:osx",
		"emulate:all": "npm run build:all && npm run run:all",
		"serve": "cordova serve -l"
	},
	"cordova": {
		"platforms": [
			"ios",
			"android",
			"electron",
			"osx",
			"windows"
		],
		"plugins": {
			"cordova-plugin-whitelist": {}
		}
	},
	"dependencies": {
		"cordova-android": "^8.0.0",
		"cordova-electron": "^1.1.0",
		"cordova-ios": "^5.0.1",
		"cordova-osx": "^5.0.0",
		"cordova-plugin-whitelist": "^1.3.4",
		"cordova-windows": "^7.0.0"
	},
	"devDependencies": {}
}