<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cisco.webex.teams" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova Webex Teams</name>
    <description>
        Cordova project with Webexteams web Components
    </description>
    <author email="arash.koush@gmail.com" href="https://github.com/akoushke">
        Arash Koushkebaghi
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="DefaultVolumeStream" value="media" />
    <preference name="ErrorUrl" value="error-page.html" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.4" />
</widget>
